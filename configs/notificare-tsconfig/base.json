{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "lib": [
      "DOM",
      "ES2017"
    ]
  },
  "exclude": [
    "dist"
  ]
}
