{
  "name": "@notificare/push",
  "version": "3.0.0-beta.1",
  "description": "The push package for the 'notificare' umbrella package.",
  "author": "Notificare <info@notificare.com> (https://notificare.com)",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "browser": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "esm5": "dist/esm5/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "sw-main": "dist/sw/commonjs/index.js",
  "sw-browser": "dist/sw/esm/index.js",
  "exports": {
    ".": {
      "node": "./dist/commonjs/index.js",
      "browser": "./dist/esm/index.js",
      "module": "./dist/esm/index.js",
      "default": "./dist/commonjs/index.js"
    },
    "./css/notificare-push.css": {
      "default": "./css/notificare-push.css"
    },
    "./sw": {
      "node": "./dist/sw/commonjs/index.js",
      "default": "./dist/sw/esm/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist/",
    "dev": "rollup -c -w",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "css",
    "sw/package.json"
  ],
  "dependencies": {
    "@notificare/core": "^3.0.0-beta.1",
    "@notificare/logger": "^3.0.0-beta.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@notificare/eslint-config": "*",
    "@notificare/rollup": "*",
    "@notificare/tsconfig": "*",
    "typescript": "^4.9.4"
  }
}
