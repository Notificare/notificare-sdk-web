{
  "name": "@notificare/web-push",
  "version": "3.0.0-beta.1",
  "description": "The push package for the 'notificare-web' umbrella package.",
  "author": "Notificare <info@notificare.com> (https://notificare.com)",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "browser": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "esm5": "dist/esm5/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "sw-main": "dist/sw/commonjs/index.js",
  "sw-browser": "dist/sw/esm/index.js",
  "exports": {
    ".": {
      "node": "./dist/commonjs/index.js",
      "browser": "./dist/esm/index.js",
      "module": "./dist/esm/index.js",
      "default": "./dist/commonjs/index.js"
    },
    "./dist/notificare-push.css": {
      "default": "./dist/notificare-push.css"
    },
    "./sw": {
      "node": "./dist/sw/commonjs/index.js",
      "default": "./dist/sw/esm/index.js"
    }
  },
  "scripts": {
    "build": "yarn build:sources && yarn build:styles",
    "build:sources": "rollup -c",
    "build:styles": "postcss css/notificare-push.css --dir dist",
    "clean": "rm -rf dist/",
    "dev": "yarn dev:sources & yarn dev:styles",
    "dev:sources": "rollup -c -w",
    "dev:styles": "postcss css/notificare-push.css --dir dist -w",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "sw/package.json"
  ],
  "dependencies": {
    "@notificare/web-core": "^3.0.0-beta.1",
    "@notificare/web-logger": "^3.0.0-beta.1",
    "@notificare/web-ui": "^3.0.0-beta.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@notificare/eslint-config": "*",
    "@notificare/rollup": "*",
    "@notificare/tsconfig": "*",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
