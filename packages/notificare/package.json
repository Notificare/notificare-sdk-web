      },
      "default": "./push-ui/dist/esm/index.esm.js"
    },
    "./push-ui/push-ui.css": {
      "default": "./push-ui/push-ui.css"
    },
    "./user-inbox": {
      "types": "./user-inbox/dist/user-inbox/index.d.ts",
      "node": {
        "require": "./user-inbox/dist/index.cjs.js",
        "import": "./user-inbox/dist/index.mjs"
      },
      "browser": {
        "require": "./user-inbox/dist/index.cjs.js",
        "import": "./user-inbox/dist/esm/index.esm.js"
      },
      "default": "./user-inbox/dist/esm/index.esm.js"
    }
  },
  "dependencies": {
    "@notificare/web-assets": "^3.0.0-beta.1",
    "@notificare/web-core": "^3.0.0-beta.1",
    "@notificare/web-geo": "^3.0.0-beta.1",
    "@notificare/web-in-app-messaging": "^3.0.0-beta.1",
    "@notificare/web-inbox": "^3.0.0-beta.1",
    "@notificare/web-push": "^3.0.0-beta.1",
    "@notificare/web-push-ui": "^3.0.0-beta.1",
    "@notificare/web-user-inbox": "^3.0.0-beta.1"
  },
  "devDependencies": {
    "@notificare/eslint-config": "*",
    "@notificare/rollup": "*",
    "@notificare/tsconfig": "*",
    "typescript": "^5.1.6"
  },
  "components": [
    "core",
    "assets",
    "geo",
    "in-app-messaging",
    "inbox",
    "push",
    "push/sw",
    "push-ui",
    "user-inbox"
  ]
}
